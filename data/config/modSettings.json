{
  "wisp_trophy-planet": {
    "showShipsInStorage": true,
    "showShipsForSale": true,
    # The scale of sprites you own, after any normalization.
    "storedSpriteScaleModifier": 1.2,
    # The scale of the sprite for sale, after any normalization.
    "forSaleSpriteScaleModifier": 0.8,
    # A higher number makes smaller ships larger and larger ships smaller.
    # 0 for no normalization.
    "normalizingAmount": 0.80,
    # The transparency of the sprites, between 0 and 1.
    "alphaMult": 0.2,
    # Whether or not to fade in on zoom for ships you own.
    "shouldFadeOnZoomInForStorage": false,
    # Whether or not to fade in on zoom for ships for sale.
    "shouldFadeOnZoomInForSale": true,
    # The key to toggle this mod. Default=P
    "toggleHotkey": "P"
  }
}